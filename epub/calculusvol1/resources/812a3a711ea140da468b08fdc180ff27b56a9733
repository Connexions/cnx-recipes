<section data-depth="1" id="fs-id1165043397752"><h3 data-type="title">Formulas from Trigonometry</h3>
<section data-depth="2" id="fs-id1165042985823"><h4 data-type="title">Right-Angle Trigonometry</h4>
<p id="fs-id1165042320749"><math xmlns="http://www.w3.org/1998/Math/MathML" display="inline"><semantics><mrow><mrow><mtable><mtr><mtd columnalign="left"><mtext>sin</mtext><mspace width="0.1em"/><mi>&#952;</mi><mo>=</mo><mfrac><mrow><mtext>opp</mtext></mrow><mrow><mtext>hyp</mtext></mrow></mfrac></mtd><mtd/><mtd/><mtd columnalign="left"><mtext>csc</mtext><mspace width="0.1em"/><mi>&#952;</mi><mo>=</mo><mfrac><mrow><mtext>hyp</mtext></mrow><mrow><mtext>opp</mtext></mrow></mfrac></mtd></mtr><mtr><mtd columnalign="left"><mtext>cos</mtext><mspace width="0.1em"/><mi>&#952;</mi><mo>=</mo><mfrac><mrow><mtext>adj</mtext></mrow><mrow><mtext>hyp</mtext></mrow></mfrac></mtd><mtd/><mtd/><mtd columnalign="left"><mtext>sec</mtext><mspace width="0.1em"/><mi>&#952;</mi><mo>=</mo><mfrac><mrow><mtext>hyp</mtext></mrow><mrow><mtext>adj</mtext></mrow></mfrac></mtd></mtr><mtr><mtd columnalign="left"><mtext>tan</mtext><mspace width="0.1em"/><mi>&#952;</mi><mo>=</mo><mfrac><mrow><mtext>opp</mtext></mrow><mrow><mtext>adj</mtext></mrow></mfrac></mtd><mtd/><mtd/><mtd columnalign="left"><mtext>cot</mtext><mspace width="0.1em"/><mi>&#952;</mi><mo>=</mo><mfrac><mrow><mtext>adj</mtext></mrow><mrow><mtext>opp</mtext></mrow></mfrac></mtd></mtr></mtable></mrow></mrow><annotation-xml encoding="MathML-Content"><mrow><mtable><mtr><mtd columnalign="left"><mtext>sin</mtext><mspace width="0.1em"/><mi>&#952;</mi><mo>=</mo><mfrac><mrow><mtext>opp</mtext></mrow><mrow><mtext>hyp</mtext></mrow></mfrac></mtd><mtd/><mtd/><mtd columnalign="left"><mtext>csc</mtext><mspace width="0.1em"/><mi>&#952;</mi><mo>=</mo><mfrac><mrow><mtext>hyp</mtext></mrow><mrow><mtext>opp</mtext></mrow></mfrac></mtd></mtr><mtr><mtd columnalign="left"><mtext>cos</mtext><mspace width="0.1em"/><mi>&#952;</mi><mo>=</mo><mfrac><mrow><mtext>adj</mtext></mrow><mrow><mtext>hyp</mtext></mrow></mfrac></mtd><mtd/><mtd/><mtd columnalign="left"><mtext>sec</mtext><mspace width="0.1em"/><mi>&#952;</mi><mo>=</mo><mfrac><mrow><mtext>hyp</mtext></mrow><mr