</div>
<p id="fs-id1170572572267">Suppose that <math xmlns="http://www.w3.org/1998/Math/MathML" display="inline"><semantics><mrow><mrow><mstyle displaystyle="true"><mrow><msubsup><mo>&#8747;</mo><mn>0</mn><mn>4</mn></msubsup><mrow><mi>f</mi><mrow><mo>(</mo><mi>x</mi><mo>)</mo></mrow><mi>d</mi><mi>x</mi></mrow></mrow></mstyle><mo>=</mo><mn>5</mn></mrow></mrow><annotation-xml encoding="MathML-Content"><mrow><mstyle displaystyle="true"><mrow><msubsup><mo>&#8747;</mo><mn>0</mn><mn>4</mn></msubsup><mrow><mi>f</mi><mrow><mo>(</mo><mi>x</mi><mo>)</mo></mrow><mi>d</mi><mi>x</mi></mrow></mrow></mstyle><mo>=</mo><mn>5</mn></mrow></annotation-xml></semantics></math> and <math xmlns="http://www.w3.org/1998/Math/MathML" display="inline"><semantics><mrow><mrow><mstyle displaystyle="true"><mrow><msubsup><mo>&#8747;</mo><mn>0</mn><mn>2</mn></msubsup><mrow><mi>f</mi><mrow><mo>(</mo><mi>x</mi><mo>)</mo></mrow><mi>d</mi><mi>x</mi></mrow></mrow></mstyle><mo>=</mo><mn>&#8722;3</mn><mo>,</mo></mrow></mrow><annotation-xml encoding="MathML-Content"><mrow><mstyle displaystyle="true"><mrow><msubsup><mo>&#8747;</mo><mn>0</mn><mn>2</mn></msubsup><mrow><mi>f</mi><mrow><mo>(</mo><mi>x</mi><mo>)</mo></mrow><mi>d</mi><mi>x</mi></mrow></mrow></mstyle><mo>=</mo><mn>&#8722;3</mn><mo>,</mo></mrow></annotation-xml></semantics></math> and <math xmlns="http://www.w3.org/1998/Math/MathML" display="inline"><semantics><mrow><mrow><mstyle displaystyle="true"><mrow><msubsup><mo>&#8747;</mo><mn>0</mn><mn>4</mn></msubsup><mrow><mi>g</mi><mrow><mo>(</mo><mi>x</mi><mo>)</mo></mrow><mi>d</mi><mi>x</mi></mrow></mrow></mstyle><mo>=</mo><mn>&#8722;1</mn></mrow></mrow><annotation-xml encoding="MathML-Content"><mrow><mstyle displaystyle="true"><mrow><msubsup><mo>&#8747;</mo><mn>0</mn><mn>4</mn></msubsup><mrow><mi>g</mi><mrow><mo>(</mo><mi>x</mi><mo>)</mo></mrow><mi>d</mi><mi>x</mi></mrow></mrow></mstyle><mo>=</mo><mn>&#8722;1</mn></mrow></annotation-xml></semantics></math> and <math xmlns="http://www.w3.org/1998/Math/MathML" 