=</mo><mstyle displaystyle="true"><mrow><msubsup><mo stretchy="false">&#8747;</mo><mi>a</mi><mi>b</mi></msubsup><mrow><mrow><mo>(</mo><mrow><mn>2</mn><mi>&#960;</mi><mi>x</mi><mi>f</mi><mo stretchy="false">(</mo><mi>x</mi><mo stretchy="false">)</mo></mrow><mo>)</mo></mrow></mrow></mrow></mstyle><mi>d</mi><mi>x</mi></mtd></mtr><mtr><mtd/><mtd columnalign="left"><mo>=</mo><mstyle displaystyle="true"><mrow><msubsup><mo stretchy="false">&#8747;</mo><mn>0</mn><mn>2</mn></msubsup><mrow><mrow><mo>(</mo><mrow><mn>2</mn><mi>&#960;</mi><mi>x</mi><mrow><mo>(</mo><mrow><mn>2</mn><mi>x</mi><mo>&#8722;</mo><msup><mi>x</mi><mn>2</mn></msup></mrow><mo>)</mo></mrow></mrow><mo>)</mo></mrow></mrow></mrow></mstyle><mi>d</mi><mi>x</mi><mo>=</mo><mn>2</mn><mi>&#960;</mi><mstyle displaystyle="true"><mrow><msubsup><mo stretchy="false">&#8747;</mo><mn>0</mn><mn>2</mn></msubsup><mrow><mrow><mo>(</mo><mrow><mn>2</mn><msup><mi>x</mi><mn>2</mn></msup><mo>&#8722;</mo><msup><mi>x</mi><mn>3</mn></msup></mrow><mo>)</mo></mrow></mrow></mrow></mstyle><mi>d</mi><mi>x</mi></mtd></mtr><mtr><mtd/><mtd columnalign="left"><mo>=</mo><msubsup><mrow><mrow><mn>2</mn><mi>&#960;</mi><mrow><mo>[</mo><mrow><mfrac><mrow><mn>2</mn><msup><mi>x</mi><mn>3</mn></msup></mrow><mn>3</mn></mfrac><mo>&#8722;</mo><mfrac><mrow><msup><mi>x</mi><mn>4</mn></msup></mrow><mn>4</mn></mfrac></mrow><mo>]</mo></mrow></mrow><mspace width="0.2em"/><mo>|</mo></mrow><mn>0</mn><mn>2</mn></msubsup><mo>=</mo><mfrac><mrow><mn>8</mn><mi>&#960;</mi></mrow><mn>3</mn></mfrac><mspace width="0.2em"/><msup><mtext>units</mtext><mn>3</mn></msup><mtext>.</mtext></mtd></mtr></mtable></annotation-xml></semantics></math></div>
