<tr valign="top">
<td data-valign="middle" data-align="center"><strong data-effect="bold"><math xmlns="http://www.w3.org/1998/Math/MathML" display="inline"><semantics><mrow><mrow><msup><mi>x</mi><mn>3</mn></msup></mrow></mrow><annotation-xml encoding="MathML-Content"><mrow><msup><mi>x</mi><mn>3</mn></msup></mrow></annotation-xml></semantics></math></strong></td>
<td data-valign="middle" data-align="center"><math xmlns="http://www.w3.org/1998/Math/MathML" display="inline"><semantics><mrow><mrow><mn>125</mn></mrow></mrow><annotation-xml encoding="MathML-Content"><mrow><mn>125</mn></mrow></annotation-xml></semantics></math></td>
<td data-valign="middle" data-align="center"><math xmlns="http://www.w3.org/1998/Math/MathML" display="inline"><semantics><mrow><mrow><mn>1000</mn></mrow></mrow><annotation-xml encoding="MathML-Content"><mrow><mn>1000</mn></mrow></annotation-xml></semantics></math></td>
<td data-valign="middle" data-align="center"><math xmlns="http://www.w3.org/1998/Math/MathML" display="inline"><semantics><mrow><mrow><mn>3375</mn></mrow></mrow><annotation-xml encoding="MathML-Content"><mrow><mn>3375</mn></mrow></annotation-xml></semantics></math></td>
