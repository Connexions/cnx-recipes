<hack-root xmlns="http://cnx.rice.edu/cnxml">
    <!-- START:Chapter -->
    <!-- START:Page -->
    <!-- Markup starts here -->
    <section class="key-equations">
        <title>Najważniejsze wzory</title>
        <table class="unnumbered unstyled" summary="Tabela przedstawia wzór na obliczenie niepewności względnej. Niepewność względna jest równa delta A przez A razy 100 procent.">
            <label/>
            <tgroup cols="2">
                <colspec colnum="1" colname="c1"/>
                <colspec colnum="2" colname="c2"/>
                <tbody>
                    <row>
                        <entry align="left">Niepewność względna</entry>
                        <entry align="left">
                            <m:math>
                                <m:mtext>Niepewność względna</m:mtext>
                                <m:mo>=</m:mo>
                                <m:mfrac>
                                    <m:mrow>
                                        <m:mi mathvariant="normal">Δ</m:mi>
                                        <m:mi>A</m:mi>
                                    </m:mrow>
                                    <m:mi>A</m:mi>
                                </m:mfrac>
                                <m:mo>·</m:mo>
                                <m:mn>100%</m:mn>
                            </m:math>
                        </entry>
                    </row>
                </tbody>
            </tgroup>
        </table>
    </section>
    <!-- END:Page -->
    <!-- END:Chapter -->
